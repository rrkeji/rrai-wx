@import '/iconfont.wxss';

.iconf {
  font-family: "iconfont" !important;
  font-size: inherit;
  font-style: normal;
}

.tabBar {
  position: fixed;
  width: 100%;
  height: 140rpx;
  bottom: 0;
  font-size: 20rpx;
  color: #8A8A8A;
  z-index: 99;
  border-top: 2rpx solid #dedede
}

.shadow {
  box-shadow: 0 1rpx 6rpx rgba(0, 0, 0, 0.1);
}


.bg-white {
  background-color: #fff;
  color: #666;
}

.shadow .bg-green {
  box-shadow: 6rpx 6rpx 8rpx rgba(48, 156, 63, 0.2);
}

.cu-bar.tabbar .action.add-action .icon-skinfill,
.cu-bar.tabbar .action.add-action .icon-close {
  position: absolute;
  width: 70rpx;
  z-index: 2;
  height: 70rpx;
  border-radius: 50%;
  line-height: 70rpx;
  font-size: 50rpx;
  top: -35rpx;
  left: 0;
  right: 0;
  margin: auto;
  padding: 0;
}

.cu-bar.tabbar .action.add-action::after {
  content: "";
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  top: -50rpx;
  left: 0;
  right: 0;
  margin: auto;
  box-shadow: 0 -3rpx 8rpx rgba(0, 0, 0, 0.08);
  border-radius: 50rpx;
  background-color: inherit;
  z-index: 0;
}

.cu-bar.tabbar .action.add-action::before {
  content: "";
  position: absolute;
  width: 100rpx;
  height: 30rpx;
  bottom: 30rpx;
  left: 0;
  right: 0;
  margin: auto;
  background-color: inherit;
  z-index: 1;
}

.cu-btn {
  position: relative;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 0 30rpx;
  font-size: 28rpx;
  height: 64rpx;
  line-height: 1;
  text-align: center;
  text-decoration: none;
  overflow: visible;
  margin-left: initial;
  transform: translate(0rpx, 0rpx);
  margin-right: initial;
}

.cu-btn::after {
  display: none;
}

.cu-bar {
  display: flex;
  position: relative;
  align-items: center;
  min-height: 100rpx;
  justify-content: space-between;
}

.cu-bar .action {
  display: flex;
  align-items: center;
  height: 100%;
  justify-content: center;
  max-width: 100%;
}

.cu-bar.tabbar .action .icon-add {
  width: 100rpx;
  position: relative;
  display: block;
  height: auto;
  margin: 0 auto 10rpx;
  text-align: center;
  font-size: 40rpx;
}

.cu-bar.tabbar {
  padding: 0;
  z-index: 0;
  height: calc(100rpx + env(safe-area-inset-bottom) / 2);
  padding-bottom: calc(env(safe-area-inset-bottom) / 2);
}


.cu-bar {}

.cu-bar .action:first-child {
  margin-left: 30rpx;
  font-size: 30rpx;
}

.cu-bar.tabbar .action {
  font-size: 22rpx;
  position: relative;
  flex: 1;
  text-align: center;
  padding: 0;
  display: block;
  height: auto;
  line-height: 1;
  margin: 0;
  background-color: inherit;
  overflow: initial;
}

.cu-bar.tabbar .action.add-action {
  position: relative;
  z-index: 2;
  padding-top: 50rpx;
}


.bottom-dialog {
  position: absolute;
  left: 0rpx;
  bottom: 0rpx;
}

.bottom-dialog-bg {
  position: absolute;
  width: 100vw;
  height: 100vh;
  top: calc(-100vh + 12rpx);
  left: 0rpx;
  background-color: #444444;
  opacity: .8;
}

.bottom-dialog .bottom-dialog-body {
  position: absolute;
  width: 92vw;
  left: 4vw;
  bottom: 160rpx;
  border-radius: 20rpx;
  z-index: 100;
  background-color: #FFFFFF;
  padding: 20rpx 20rpx;
}

.bottom-dialog .brain-list {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

.bottom-dialog .brain-item {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  padding: 10rpx;
  margin: 10rpx 20rpx;
  box-sizing: border-box;
}

.bottom-dialog .brain-item.selected {
  border: 4rpx solid #499CFB;
  border-radius: 16rpx;
}

.bottom-dialog .brain-item-title {
  color: #474747;
  line-height: 40rpx;
  font-size: 26rpx;
  font-weight: 600;
}

.bottom-dialog .brain-item-icon {
  width: 160rpx;
  height: 160rpx;
}

.bottom-dialog .brain-item-current {
  background-color: #CBE4FF;
  color: #0062DB;
  width: 100rpx;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 14rpx;
  font-weight: 500;
  font-size: 24rpx;
}

.bottom-dialog .brain-item-switch {
  background-color: #A7E9BB;
  color: #0062DB;
  margin-top: 10rpx;
}

.icon-homefill,
.icon-skinfill,
.icon-search,
.icon-skin,
.icon-discover,
.icon-question,
.icon-message,
.icon-selection,
.icon-close,
.icon-my {
  width: 100rpx;
  position: relative;
  display: block;
  height: auto;
  margin: 0 auto 10rpx;
  text-align: center;
  font-size: 40rpx;
}

.icon-close:before {
  content: "\e646";
}

.icon-homefill:before {
  content: "\e6bb";
}

.icon-search:before {
  content: "\e65c";
}

.icon-my:before {
  content: "\e78b";
}

.icon-discover:before {
  content: "\e67e";
}

.icon-question:before {
  content: "\e691";
}

.icon-skinfill:before {
  content: "\e7cc";
}

.icon-skin:before {
  content: "\e7cd";
}

.icon-selection:before {
  content: "\e6ce";
}

.icon-message:before {
  content: "\e6bc";
}

