page{
  overflow: hidden;
  height: 100%;
}
.contain{
  position: relative;
box-sizing: border-box;
width: 100vw;
height: 100vh;
}
.contain-photo{
  width: 100%;
  height: 100%;
}
image{
  width: 100%;
  height: 100%;
}
.person{
  width: 100%;
  height: 100%;
  position: absolute;
  font-size: 150rpx;
  top: 200rpx;
  color: #fff;
}
.person image{
  width: 100%;
  height: 60%;
}
